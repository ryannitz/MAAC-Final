        <arguments>
          <argument>
            <kind>1</kind>
            <string>//proximity checks on buildings

//player collision mask for buildings:
x1 = x-25;
y1 = y;
x2 = x+50;
y2 = y+50;
colide = false;

//police station

obj = obj_policeStation;
if(collision_rectangle(x-25, y, x+50, y+50, obj, false, false)){
    //highlight the building 
    //obj_policeStationTop.image_index = 2
    //obj_policeStationTop.sprite_index = spr_policeStationSPLIT;
    obj.image_index = 1;
    obj.sprite_index = spr_policeStation;
    
}else{
    //obj_policeStationTop.image_index = 0
    //obj_policeStationTop.sprite_index = spr_policeStationSPLIT;
    obj.image_index = 0;
    obj.sprite_index = spr_policeStation;
} 

//hospital
obj = obj_hospital;
if(collision_rectangle(x-25, y, x+50, y+50, obj, false, false)){
    obj.image_index = 1;
    obj.sprite_index = spr_hospital;
}else{
    obj.image_index = 0;
