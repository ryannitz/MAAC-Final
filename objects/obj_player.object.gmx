            <string>//potential issue: If two selectable buildings are being triggered by the collision 
//solution: use game maker method to grab distance between objects and highlight the closest one. 
level[0] = maryJ_level;
level[1] = cocaine_level;
level[2] = heroin_level;
level[3] = acid_level;

active[0] = maryJ_isActive;
active[1] = cocaine_isActive;
active[2] = heroin_isActive;
active[3] = acid_isActive;

showing[0] = maryJ_isShown;
showing[1] = cocaine_isShown;
showing[2] = heroin_isShown;
showing[3] = acid_isShown;

var length = 4;

if(collision_rectangle(x-25, y, x+50, y+50, obj_policeStation, false, false)){
    //check players drug levels and criminal experience. (Street cred)    
    show_debug_message("pressed on the police station");
    
    var arrested = false;
    for(p = 0; p &lt; 4; p++){
        //if(level[p] &gt; 75 || active[p] == true || street_credit &gt; 25){
            //arrested
        //}
    
    
        if(level[p] &gt; 75){
            show_debug_message("Arrested for drug use");
        }else if(active[p] == true){
            show_debug_message("Arrested for under the influence in public");
        }else if(street_credit &gt; 25){
            show_debug_message("Arrested for known drug affiliation");
        }
    }
}

if(collision_rectangle(x-25, y, x+50, y+50, obj_hospital, false, false)){
    //check players drug level and decide whether they get help or thrown out. 
    show_debug_message("Pressed on the hospital");
    
    var doTreatment = true;
    
    //if any of the drugs is above 80, do not continue
    for(i = 0; i &lt; 4; i++){
        if(level[i] &gt; 80 || active[i] == true){
            doTreatment = false;
        }
    }
    
    if(doTreatment){
        var treated = false;
        for(j = 0; j &lt; 4; j++){
            if(level[j] &lt; 60 &amp;&amp; level[j] &gt; 50 &amp;&amp; showing[j] == true){
                switch(j){
                    case 0:
                        show_debug_message("Cured MaryJ");
                    case 1: 
                        cocaine_isShown = false; 
                        cocaine_level = 50; 
                        player_dir = 1;  
                        show_debug_message("Cured cocaine");
                    case 2:
                        heroin_isShown = false;
                        heroin_level = 50;
                        show_debug_message("Cured heroin");
                    case 3:
                        acid_isShown = false;
                        acid_level = 50;
                        show_debug_message("Cured acid");
                }
                treated = true;
            }
        }
        
        if(!treated){
            //so something about how nothing can be cured
            show_debug_message("Cannot be treated")
        }
        
    }else{
        //do something about how one of the drugs is too high
        show_debug_message("Too High on drugs to be treated")
    }
}

