        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(keyboard_check(vk_left) || keyboard_check(ord('A'))){
    image_speed = 1/2;
    //image_speed = (player_speed/room_speed);
    if(image_index == 0){
        image_index = 1;
    }
    sprite_index = spr_playerFL;
    
}else if(keyboard_check(vk_right) || keyboard_check(ord('D'))){
    image_speed = 1/2;
    //image_speed = (player_speed/room_speed);
    if(image_index == 0){
        image_index = 1/2;
    }
    sprite_index = spr_playerFR;
    
}else if(keyboard_check(vk_up) || keyboard_check(ord('W'))){
    image_speed = 1/2;
    //image_speed = (player_speed/room_speed);
    if(image_index == 0){
        image_index = 1;
    }
    sprite_index = spr_playerFF;
    
}else if(keyboard_check(vk_down) || keyboard_check(ord('S'))){
    image_speed = 1/2;
    //image_speed = (player_speed/room_speed);
    if(image_index == 0){
        image_index = 1;
    }
    sprite_index = spr_playerFB;
    
}else if(keyboard_check(vk_nokey)){
    //will stop the animation and set to the idle sub-image. 
    image_speed = 0;
    image_index = 0;
}
</string>
